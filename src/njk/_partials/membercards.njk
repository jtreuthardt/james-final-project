<section>
<div class="row">
{% for member in data.members %}
  <div class="col-sm-3 col-md-6 col-lg-6">
    <div class="card">
      <img class="img-fluid" src= {{ member.imagebw }} class="card-img-top" alt="...">
      <div class="card-body">
        <h5 class="card-title"> {{ member.name }} </h5>
        <p class="card-text"> {{ member.instruments }}</p>
        <a href="{{ member.slug }}.html" class="btn btn-primary">Learn More</a>
      </div>
    </div>
  </div>
{% endfor %}
</div>
</section>
